	<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
http://www.springframework.org/schema/webflow/spring-webflow.xsd">

	
	<view-state id="addtocart" view="Cart.jsp">
	   <transition on="page1" to="Address"/>
	</view-state>
	
	
	<view-state id="Address" view="Address.jsp">
	<transition on="event2" to="Shipping"></transition>
	</view-state>
	
	<view-state id="Shipping" view="ShippingAddress.jsp">
	<transition on="event3" to="Summery"></transition>	
	</view-state>
	
	<view-state id="Summery" view="OrderConfirmation.jsp">
	<transition on="event4" to="thankyou"></transition>	
	</view-state>

	<view-state id="thankyou" view="Thankyou.jsp">
	
	</view-state>
	
</flow>
